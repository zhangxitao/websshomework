<%- include header.ejs%>
    <div class="text-center page-title">用户登录</div>
    <div class="container">
        <div class="row">
            <div class="col-xs-4 col-xs-offset-4">
                <form method="post">

                    <!--用户名-->
                    <div class="form-group">
                        <input type="text" class="form-control" name="username" palceholder="用户名">
                    </div>

                    <!--密码-->
                    <div class="form-group">
                        <input type="password" class="form-control" name="password" palceholder="密码">
                    </div>


                    <!--注册-->
                    <button type="submit" class="btn btn-default center-block">登录</button>
                </form>
            </div>
        </div>
    </div>

