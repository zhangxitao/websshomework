<%- include header.ejs%>
<div class="container">
    <div class="row">
        <table class="table table-striped">
            <caption>我的笔记列表</caption>
            <thead>
            <tr>
                <th>标题</th>
                <th>标签</th>
                <th>日期</th>
            </tr>
            </thead>
            <tbody>
            <!--<tr>
                <td>这是笔记标题1</td>
