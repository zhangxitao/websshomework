                <td>这是笔记日期1</td>
            </tr>
            <tr>
                <td>这是笔记标题2</td>
                <td>这是笔记标签2</td>
                <td>这是笔记日期2</td>
            </tr>
            <tr>
                <td>这是笔记标题3</td>
                <td>这是笔记标签3</td>
                <td>这是笔记日期3</td>
            </tr>-->

            <% notes.forEach(function(note){%>
                <tr>
                    <td>
                        <a href="/detail/<%= note._id%>"><%= note.title%></a>
                    </td>
                    <td><%= note.tag %></td>
                    <td><%= note.createTime%>></td>
                </tr>
            <%})%>


            </tbody>
        </table>
    </div>
</div>

